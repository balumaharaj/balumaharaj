package com;

public class threadinfo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
  Thread t=Thread.currentThread();
  System.out.println(t);
  t.setName("my thread");
  t.setPriority(1);
  System.out.println(t);
  //t.setPriority(Thread.MAX-PRIORITY);
  System.out.println(t);
  
	}

}
